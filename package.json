{
  "name": "endre.chat",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "test:unit": "vitest",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "start": "node build/index.js",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:reset": "rm -rf .db && npm run db:migrate",
    "db:studio": "prisma studio",
    "n:db:migrate": "nix-shell --command 'npx prisma migrate dev'",
    "n:db:seed": "nix-shell --command 'npx prisma db seed'",
    "n:db:reset": "rm -rf .db && npm run n:db:migrate",
    "n:db:studio": "nix-shell --command 'npx prisma studio'",
    "n:dev": "nix-shell --command 'npm run dev'"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^4.6.0",
    "blobs": "^2.3.0-beta.2",
    "dateformat": "^5.0.3",
    "lodash": "^4.17.21",
    "openai": "^3.2.1",
    "rxjs": "^7.8.0",
    "svelte-kit-cookie-session": "^3.3.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-node": "^1.2.3",
    "@sveltejs/kit": "^1.5.0",
    "@types/lodash": "^4.14.191",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "prisma": "4.6.0",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "unocss": "0.50.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.3"
  }
}
